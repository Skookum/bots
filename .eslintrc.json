{
    "extends": "google",
    "env": {
        "node": true
    },
    "rules": {
        "no-var": "off",
        "require-jsdoc": "off",
        "new-cap": "off",
        "prefer-rest-params": "off",
        "object-curly-spacing": [ "error", "always" ],
        "max-len": [ "error", 100 ]
    }
}