body
  font-family helvetica
  text-align center
ul
  text-align center
  padding 0
li
  text-decoration none
  display inline-block
  padding 10px
li.selected
  background-color red
canvas
  border 1px solid black
  margin auto
  display block
#newgame, #newbot
  background-color orange
  margin 10px auto
  padding 2px
  border-radius 2px
  text-align center
  width 100px
  cursor pointer
  display inline-block
